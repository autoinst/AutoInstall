[
    {
        "file": "/inst.json",
        "name": "安装器配置",
        "notice": "安装器基本配置，删除run.sh重启将重新安装",
        "exps": [
            {
                "type": "string",
                "name": "服务器版本",
                "notice": "预计的服务器版本",
                "exp": "\"version\": \"(.+)\""
            },
            {
                "type": "select",
                "name": "加载器",
                "notice": "选择需要的加载器",
                "exp": "\"loader\": \"(forge|fabric|neoforge)\"",
                "options": [
                    {
                        "label": "Forge",
                        "value": "forge"
                    },
                    {
                        "label": "Fabric",
                        "value": "fabric"
                    },
                    {
                        "label": "NeoForge",
                        "value": "neoforge"
                    }
                ]
            },
            {
                "type": "string",
                "name": "加载器版本",
                "notice": "修改加载器版本",
                "exp": "\"loaderVersion\": \"(.+)\""
            },
            {
                "type": "select",
                "name": "下载源",
                "exp": "\"download\": \"(bmclapi|none)\"",
                "options": [
                    {
                        "label": "BMCLAPI",
                        "value": "bmclapi"
                    },
                    {
                        "label": "默认",
                        "value": "none"
                    }
                ]
            },
            {
                "type": "string",
                "name": "并发数",
                "notice": "同时下载线程数",
                "exp": "\"maxconnections\": \"(.+)\""
            }
        ]
    }
]