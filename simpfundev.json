[
    {
        "file": "/server.properties",
        "name": "服务端基础配置",
        "notice": "MC服务端基础配置，重启生效",
        "exps": [
            {
                "type": "string",
                "name": "服务器版本",
                "notice": "修改服务器的显示名称",
                "exp": "\"version\": \"(.+)\","
            },
            {
                "type": "select",
                "name": "加载器",
                "notice": "选择玩家的游戏模式",
                "exp": "\"loader\": \"(forge|fabric|neoforge)\",",
                "options": [
                    {
                        "label": "Forge",
                        "value": "forge"
                    },
                    {
                        "label": "Fabric",
                        "value": "fabric"
                    },
                    {
                        "label": "NeoForge",
                        "value": "neoforge"
                    }
                ]
            },
            {
                "type": "string",
                "name": "加载器版本",
                "notice": "修改服务器的显示名称",
                "exp": "\"loaderVersion\": \"(.+)\","
            },
            {
                "type": "select",
                "name": "下载源",
                "exp": "\"download\": \"(bmclapi|none)\"",
                "options": [
                    {
                        "label": "BMCLAPI",
                        "value": "bmclapi"
                    },
                    {
                        "label": "默认",
                        "value": "none"
                    }
                ]
            }
        ]
    }
]